start: loadI @n => r_n
       loadI 1 => r_x
       loadI 1 => r_y
       loadI 1 => r_z
       loadI 1 => r_one
while: cmp_GT r_n,r_one => r_cmp
       cbr r_cmp -> body,end
body:  add r_x,r_y => r_z
       i2i r_y => r_x
       i2i r_z => r_y
       subI r_n,1 => r_n
       jumpI -> while
end:   out "Fib: ", r_z
