start:   in "" => r_x
         loadI 0 => r_i
         loadI @alength => r_len
for:     cmp_LT r_i,r_len => r_cmp
         cbr r_cmp -> if, end
if:      i2i r_i => r_a
         multI r_a, 4 => r_a
         addI r_a, @a => r_a
         loadAO r_arp, r_a => r_ai
         out "R_AI: ", r_ai
         out "R_X: ", r_x
         cmp_EQ r_ai, r_x => r_cmp
         cbr r_cmp -> end, forend
forend:  addI r_i, 1 => r_i
         jumpI -> for
end:     out "Index: ", r_i